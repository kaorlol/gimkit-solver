    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webextension-polyfill": "^0.10.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
