{
	"compilerOptions": {
		"target": "ESNext",
		"module": "commonjs",
		"outDir": "dist",
		"strict": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"allowJs": true,
		"paths": {
            "types/*": ["./src/types/*"],
			"utils/*": ["./src/utils/*"],
        },
	},
	"include": ["./src/**/*"]
}
